@model IEnumerable<BOL.tbl_ACCESSIBILITE>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_COLLECTIVITE_TERRITORIALE.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_MOYEN_ACCES.moyen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl_NIVEAU_ACCES.nom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbl_COLLECTIVITE_TERRITORIALE.id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbl_MOYEN_ACCES.moyen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbl_NIVEAU_ACCES.nom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
